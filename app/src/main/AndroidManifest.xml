<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nksystems.cvmaker">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_icon2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomeTab"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TemplatesTab"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StatisticsTab"
            android:screenOrientation="portrait" />
        <activity
            android:name=".test"
            android:label="@string/title_activity_test"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".est"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreateResume"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity_feedback"
            android:screenOrientation="portrait" />
        <activity android:name=".activity_settings"></activity>
    </application>

</manifest>